{
  "name": "farbrengen-app",
  "version": "1.0.0",
  "description": "Farbrengen Full-Stack Application",
  "private": true,
  "scripts": {
    "install:all": "npm install --prefix server && npm install --prefix my-app",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --prefix server",
    "dev:client": "npm start --prefix my-app",
    "start:server": "npm start --prefix server",
    "start:client": "npm start --prefix my-app",
    "build": "npm run build --prefix my-app"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
